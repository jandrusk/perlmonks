CREATE TABLE `nodelet` (
  `nodelet_id` int(11) NOT NULL auto_increment,
  `nltext` text,
  `nlcode` text,
  `updateinterval` int(11) NOT NULL default '0',
  `nlgoto` int(11) default NULL,
  `parent_container` int(11) default NULL,
  `lastupdate` int(11) NOT NULL default '0',
  PRIMARY KEY  (`nodelet_id`),
  KEY `parent_container` (`parent_container`)
) ENGINE=MyISAM AUTO_INCREMENT=876674 DEFAULT CHARSET=latin1