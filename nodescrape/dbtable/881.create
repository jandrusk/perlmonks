CREATE TABLE `htmlpage` (
  `htmlpage_id` int(11) NOT NULL auto_increment,
  `pagetype_nodetype` int(11) default NULL,
  `displaytype` varchar(20) default NULL,
  `page` text,
  `parent_container` int(11) default NULL,
  `ownedby_theme` int(11) NOT NULL default '0',
  `mimetype` varchar(255) default NULL,
  PRIMARY KEY  (`htmlpage_id`),
  KEY `parent_container` (`parent_container`)
) ENGINE=MyISAM AUTO_INCREMENT=882857 DEFAULT CHARSET=latin1