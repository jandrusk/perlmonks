CREATE TABLE `rawdata` (
  `rawdata_id` int(11) NOT NULL auto_increment,
  `datatype` varchar(255) default NULL,
  `databytes` mediumblob,
  `lastedit` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  PRIMARY KEY  (`rawdata_id`)
) ENGINE=MyISAM AUTO_INCREMENT=958337 DEFAULT CHARSET=latin1