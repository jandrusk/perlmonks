CREATE TABLE `pollpopularity` (
  `polls_id` int(11) NOT NULL default '0',
  `pollday` int(11) NOT NULL default '0',
  `pollvotes` int(11) default '0',
  `comments` int(11) default '0',
  `rootvotes` int(11) default '0',
  `notevotes` int(11) default '0',
  PRIMARY KEY  (`polls_id`,`pollday`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1