CREATE TABLE `testpolls` (
  `testpolls_id` int(11) NOT NULL default '0',
  `choices` text NOT NULL,
  `numbers` text NOT NULL,
  `gimmick` int(11) NOT NULL default '0',
  `gimmickchoice` int(11) NOT NULL default '0',
  `prologue` text,
  PRIMARY KEY  (`testpolls_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1