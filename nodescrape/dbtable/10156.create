CREATE TABLE `dailystatistics` (
  `date` datetime NOT NULL default '0000-00-00 00:00:00',
  `numusers` int(11) NOT NULL default '0',
  `lu_day` int(11) NOT NULL default '0',
  `hits` int(11) NOT NULL default '0',
  `lu_week` int(11) NOT NULL default '0',
  `lu_2weeks` int(11) NOT NULL default '0',
  `lu_4weeks` int(11) NOT NULL default '0',
  `lu_ever` int(11) NOT NULL default '0',
  `totalnodes` int(11) NOT NULL default '0'
) ENGINE=MyISAM DEFAULT CHARSET=latin1