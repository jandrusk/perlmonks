CREATE TABLE `cachedresults` (
  `codetitle` varchar(240) NOT NULL default '',
  `cachekey` varchar(240) NOT NULL default '',
  `updatekey` varchar(240) NOT NULL default '',
  `resultstype` int(11) NOT NULL default '0',
  `results` text NOT NULL,
  `updateinterval` int(11) NOT NULL default '0',
  `lastupdate` datetime NOT NULL default '0000-00-00 00:00:00'
) ENGINE=MyISAM DEFAULT CHARSET=latin1