<field name="page">
&lt;div class="paneled"&gt;
[{openform}]
&lt;b&gt;Question:&lt;/b&gt;&lt;br&gt;
&lt;input type=textfield name="categorized question_title" value="[%$$NODE{title}%]" size=50 maxlength=80&gt;&lt;br&gt;&lt;BR&gt;
&lt;B&gt;Further Description:&lt;/B&gt; (Try to avoid using... make the question itself as descriptive as possible)&lt;BR&gt;
&lt;textarea name="categorized question_doctext" cols=50 rows=5&gt;[%$$NODE{doctext}%]&lt;/textarea&gt;&lt;BR&gt;
[{closeform}]
&lt;/div&gt;
</field>
