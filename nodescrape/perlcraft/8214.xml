#!/usr/bin/perl
while ((@_=split ':',($_=&lt;&gt;))&amp;&amp;($_=$_[0])) {$e?(print join
':',$_,$h{$_},($_[2])x2,'',0,0,@_[4..$#_]):($h{$_}=$_[1]); $e||=eof;}
