              &lt;tr&gt; 
                &lt;td&gt; 
                  &lt;table width=&quot;100&quot; border=&quot;1&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot; bordercolor=&quot;#000000&quot;&gt;
                    &lt;tr&gt; 
                      &lt;td&gt; 
                        &lt;div align=&quot;center&quot;&gt;ataka&lt;/div&gt;
                      &lt;/td&gt;
