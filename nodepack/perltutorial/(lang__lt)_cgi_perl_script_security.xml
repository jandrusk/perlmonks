                  &lt;table width=&quot;100%&quot; border=&quot;1&quot; cellpadding=&quot;3&quot; cellspacing=&quot;0&quot; bgcolor=&quot;#CCCCCC&quot; bordercolor=&quot;#333333&quot;&gt;
